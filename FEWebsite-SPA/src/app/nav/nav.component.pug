  button.navbar-toggler(type='button' data-toggle='collapse' data-target='#navbarsExampleDefault' aria-controls='navbarsExampleDefault' aria-expanded='false' aria-label='Toggle navigation')
    span.navbar-toggler-icon
  ul.navbar-nav.mr-auto
    li.nav-item.active
      a.nav-link(href='#') Users 
        span.sr-only (current)
    li.nav-item
      a.nav-link(href='#') Media
    li.nav-item.dropdown
      a#dropdown01.nav-link.dropdown-toggle(data-toggle='dropdown' aria-haspopup='true' aria-expanded='false') Games
      .dropdown-menu(*ngFor="let num of nums" aria-labelledby='dropdown01')
